{
  "name": "diegolunal/camagru",
  "description": "A web application for users to capture and share images with stickers.",
  "type": "project",
  "require": {
      "php": "^7.4 || ^8.0",
      "ext-mbstring": "*",
      "ext-xml": "*",
      "ext-json": "*",
      "monolog/monolog": "^2.0",
      "vlucas/phpdotenv": "^5.0",
      "guzzlehttp/guzzle": "^7.0"
  },
  "autoload": {
      "psr-4": {
          "App\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Tests\\": "tests/"
      }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}